{
    "name": "armincms/template-detail",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0", 
        "armincms/armincms": "~0.1",
        "whitecube/nova-flexible-content": "^0.1",
        "owenmelbz/nova-radio-field" : "~0.0",
        "timothyasp/nova-color-field" : "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Armincms\\TemplateDetail\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Armincms\\TemplateDetail\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
